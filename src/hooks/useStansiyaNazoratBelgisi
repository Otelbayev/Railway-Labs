export function stansiyaNazoratBelgisi(stansiyaKodni) {
  const raqamlar = stansiyaKodni.toString().split("").map(Number);
  const koeffitsientlar = [1, 2, 3, 4, 5];
  let umumiyYigindi = 0;


  for (let i = 0; i < koeffitsientlar.length; i++) {
    umumiyYigindi += raqamlar[i] * koeffitsientlar[i];
  }



  const nazoratBelgisi = umumiyYigindi % 11;

  if (nazoratBelgisi === 10) {
    const yangiKoeffitsientlar = [3, 4, 5, 6, 7];
    umumiyYigindi = 0;
    for (let i = 0; i < raqamlar.length; i++) {
      umumiyYigindi += raqamlar[i] * yangiKoeffitsientlar[i];
    }
    return umumiyYigindi % 11 === 10 ? "Noto‘g‘ri kod" : umumiyYigindi % 11;
  }

  return nazoratBelgisi;
}
